public class Item {
    private String name;
    private double price;
    private int calories;

    public Item(String name, double price, int calories) {
        this.name = name;
        this.price = price;
        this.calories = calories;
    }

    public double getPrice() {
        return price;
    }

    @Override
    public String toString() {
        return name + "- price: $" + price + " - calories: " + calories;
    }
}
